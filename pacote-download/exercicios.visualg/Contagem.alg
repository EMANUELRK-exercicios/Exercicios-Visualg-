algoritmo "Contagem"
var
   M,cont,pula:inteiro
inicio
      Escreva("Quer contar até quanto?")
      leia(cont)
      Escreva("Vai querer pular de quanto em quanto? ")
      leia(pula)

      M<-0
      Enquanto (M<cont) faca
      EscrevaL(M)
      M<- M + pula
      Fimenquanto
      Escreval( " Fim da linha " )

fimalgoritmo
