algoritmo "Combinaçoesfibonacci"
var
   t1,t2,t3, C: inteiro
procedimento proximofibonnaci (var A,B:inteiro)
var C:inteiro
inicio
       C:= A + B
           escreva (C)
           A := B
           B := C

fimprocedimento
inicio
      T1<-0
      escreva(t1)
      t2<-1
      escreva(t2)
      para C := 3 ate 15 faca
          PROXIMOFIBONNACI (T1,T2)
      
      
      fimpara



fimalgoritmo
